#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mono/node_modules/.pnpm/astro@4.13.2_@types+node@22.1.0_typescript@5.5.4/node_modules/astro/node_modules:/mono/node_modules/.pnpm/astro@4.13.2_@types+node@22.1.0_typescript@5.5.4/node_modules:/mono/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mono/node_modules/.pnpm/astro@4.13.2_@types+node@22.1.0_typescript@5.5.4/node_modules/astro/node_modules:/mono/node_modules/.pnpm/astro@4.13.2_@types+node@22.1.0_typescript@5.5.4/node_modules:/mono/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
